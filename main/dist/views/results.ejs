<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="./results.css">
</head>
<body>
    
    <div class="nav-wrapper">
        <nav class="nav">
            <form action="/return-items" method="post">
                <input type="text" name="searchbar" >
            </form>
            <div id ='title'>
                <h1>Surf-Buddy</h1>
            </div>
        </nav>
    </div>
    <main>

    <%if(arr.organic_results){%>
        <%if(arr.knowledge_graph) {%>
            <div class="graph">
                
                <h3><%- arr.knowledge_graph.title %></h1>
                <a href="<%- arr.knowledge_graph.website %>">
                    <%- arr.knowledge_graph.website %>
                </a>
                <br>
                <article>
                    <%- arr.knowledge_graph.description %>
                </article>
            </div>
            <%}%>
            
        <% for(let i of arr.organic_results) { %>
            <section>
                <div class="title-link">
                    <a href="<%= i.link %>">
                        <div class ='title-link'>
                            <%= i.displayed_link %>
                        </div>
                        <div class="title-text">
                            <h1><%= i.title %>
                            </h1>
                        </div>
                       
                    </a>                
                    
                </div>
               
                <div class="description">
                    <%= i.snippet %>
                </div>
            </section>
        <% } %>


        <div class="show">
            <h3 style='font-weight: 100;'>Show More</h3>
        </div>
     
        

     </main>
    <%}%>



    <script src="/socket.io/socket.io.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/ejs@3.1.6/ejs.min.js"></script>
    <script type = 'module' crossorigin='ananymous' src="../scripts/index.js"></script>



   
  
    
</body>
</html>